   $(function() {
   	$(".newsletters").on('submit', function(event) {
   		
   		var $form = $(this);		
   		$.ajax({
   			type: $form.attr('method'),
   			url: $form.attr('action'),
   			data: $form.serialize(),
   			success: function() {
    			$('#successmsgs').html('Thank you for your enquiry with us, <br> Our representative will get in touch with you soon.');
   		 	$('#successmsgs').addClass(' alert alert-success');
   						 			 
   			 $("#successmsgs").show();
   			 setTimeout(function() { $("#successmsgs").hide(); }, 15000);
   			 $('#newsletters form')[0].reset();			 
     			
   			}
   		});
   		event.preventDefault();		 
   	});
   });
</script>
